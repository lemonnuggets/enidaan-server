{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint ./**/*.{css,scss,sass}",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix ./",
    "fix:prettier": "prettier --write './**/*.{js,md,json}' --config ./.prettierrc",
    "fix:stylelint": "stylelint --fix ./**/*.{css,scss,sass}",
    "fix:yarn-dedupe": "yarn dedupe"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix"
    ],
    "*.{md,markdown,json}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^2.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "chalk": "^5.0.0",
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.29.1",
    "mongodb": "^4.3.1",
    "mongoose": "^6.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-refresh": "^2.1.0",
    "postcss": "^8.4.6",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "validator": "^13.7.0"
  }
}
