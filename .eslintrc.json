{
  "extends": [
    "airbnb-base",
    "plugin:node/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "implicit-arrow-linebreak": ["off"],
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-process-exit": 0
  },
  "settings": {
    "import/parser": {
      "babel-eslint": true,
      "flow": true,
      "typescript": true,
      "node": true
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
